<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lateralview.net.m2xapiv2exampleapp" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application.M2XApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.DeviceActivity"
            android:label="@string/title_activity_device" >
        </activity>
        <activity
            android:name=".activities.DistributionActivity"
            android:label="@string/title_activity_distribution" >
        </activity>
        <activity
            android:name=".activities.KeysActivity"
            android:label="@string/title_activity_keys" >
        </activity>
        <activity
            android:name=".activities.ChartsActivity"
            android:label="@string/title_activity_charts" >
        </activity>
    </application>

</manifest>
